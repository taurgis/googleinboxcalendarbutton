{
    "name": "Google Inbox - Calendar button",
    "description": "A simple plugin that adds an 'add to calendar' button below meeting invites.",
    "version": "1",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://inbox.google.com/*"],
        "js": ["main.js"]
    }],
    "icons": {
        "16": "images/icons/icon16.png",
        "48": "images/icons/icon48.png",
        "128": "images/icons/icon128.png"
    }
}
